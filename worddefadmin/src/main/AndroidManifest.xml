<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practice.worddefadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EventsAndNotificationManagerActivities.UpdateExistingEvents"
            android:parentActivityName=".EventsAndNotificationManagerActivities.EventsAndNotificationManagerActivity" />
        <activity
            android:name=".EventsAndNotificationManagerActivities.NewEvent"
            android:parentActivityName=".EventsAndNotificationManagerActivities.EventsAndNotificationManagerActivity" />
        <activity
            android:name=".EventsAndNotificationManagerActivities.UpdateExistingNotifications"
            android:parentActivityName=".EventsAndNotificationManagerActivities.EventsAndNotificationManagerActivity" />
        <activity
            android:name=".EventsAndNotificationManagerActivities.NewNotification"
            android:parentActivityName=".EventsAndNotificationManagerActivities.EventsAndNotificationManagerActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".get_help.GetHelpActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ScholarManagerActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EventsAndNotificationManagerActivities.EventsAndNotificationManagerActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UpdatePrayerTimesActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>